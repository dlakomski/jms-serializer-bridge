{
    "name": "dlakomski/jms-serializer-bridge",
    "description": "Bridge for using JMSSerializer as message serializer for SimpleBus/Asynchronous",
    "keywords": [
        "asynchronous",
        "message",
        "serialization"
    ],
    "homepage": "http://github.com/SimpleBus/JMSSerializerBridge",
    "license": "MIT",
    "authors": [
        {
            "name": "Cliff Odijk",
            "email": "cliff@jcid.nl"
        },
        {
            "name": "Ruud Kamphuis",
            "homepage": "https://github.com/ruudk"
        },
        {
            "name": "Matthias Noback",
            "email": "matthiasnoback@gmail.com",
            "homepage": "http://php-and-symfony.matthiasnoback.nl"
        }
    ],
    "type": "library",
    "require": {
        "php": "^8.2",
        "dlakomski/serialization": "^6.4",
        "jms/serializer": "^3.15"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.11",
        "phpunit/phpunit": "^11.5.46",
        "symfony/phpunit-bridge": "^6.4.23 || ^7.4",
        "symfony/yaml": "^6.4.23 || ^7.4"
    },
    "autoload": {
        "psr-4": {
            "SimpleBus\\JMSSerializerBridge\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleBus\\JMSSerializerBridge\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    },
    "conflict": {
        "jms/metadata": "<2.5.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
